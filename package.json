    "repository": {
        "type": "git",
        "url": "git+https://github.com/larssandell/Semester_3_Project_exame_2.git"
    },
    "keywords": [],
    "author": "Lars Sandell",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/larssandell/Semester_3_Project_exame_2/issues"
    },
    "homepage": "https://github.com/larssandell/Semester_3_Project_exame_2#readme",
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "eslint": "^8.27.0",
        "eslint-plugin-jest": "^27.1.5",
        "husky": "^8.0.2",
        "jest": "^29.2.0",
        "lint-staged": "^13.0.3",
        "live-server": "^1.2.2",
        "prettier": "^2.7.1",
        "sass": "^1.56.1"
    },
    "lint-staged": {
        "/src/js/*.js": [
            "prettier --write",
            "eslint --fix"
