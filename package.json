    "description": "Auction site",
    "main": "index.js",
    "scripts": {
        "build": "sass src/scss:dist/css",
        "start": "sass --watch src/scss:dist/css & live-server",
        "format": "prettier -w",
        "lint": "eslint src/**/*.js",
        "prepare": "husky install",
        "unit": "npm run test-unit",
        "test-unit": "jest",
        "test": "npm run test-e2e"
